        {
          "alias": "",
          "format": "time_series",
          "rawSql": "SELECT now() as time, sum(passed)/sum(total) * 100 as perc_passed from (\n  SELECT (select sum(case when (sec->>'passed')::boolean then 1 else 0 end)\n  from jsonb_array_elements(s.data->'security') as sec) as passed,\n  jsonb_array_length(s.data->'security') as total\n  FROM mobileAppMetrics as s\n  INNER JOIN (\n    SELECT DISTINCT a.clientId\n    FROM mobileAppMetrics as a\n    WHERE a.data->'app'->>'appId' like $appId\n    AND a.data->'device'->>'platform' like $platform\n  ) as apps ON (s.clientId = apps.clientId)\n  group by s.data, s.event_time\n  HAVING $__timeFilter(s.event_time)\n) as totals\n",
          "refId": "A"
        }
      ],
